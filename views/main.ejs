<!doctype html>

<html>
<head>
	<meta http-equiv="content-type" content="text/html; charset=utf-8"  />
	<meta name="viewport" content="width=device-width">
	<title>ideahacks 2016</title>
	<link rel="stylesheet" type="text/css" href="stylesheets/main.css" />

	<link href='https://fonts.googleapis.com/css?family=Lato:400,700' rel='stylesheet' type='text/css'>
</head>

<body>
	<h1 id="title">Beats by Myo</h1>

	<span id="launchpad">
		<div id="pad1"></div>
		<div id="pad2"></div>
		<div id="pad3"></div>
		<div id="pad4"></div>
		<div id="pad5"></div>
	</span>
	<button id="playLoop">Play Loop</button>
	<br>
	<button id="pauseLoop">Pause Loop</button>

	<div id="bottom-bar">
		<div id="show"><h3>Instruments</h3></div>
		<div id="instruments">
			<span id="instruments-grid">
				<ul style="padding: 0;">
					<li class="instruments-icon" id="active">1</li>
					<a href="/main2"><li class="instruments-icon">2</li></a>
					<li class="instruments-icon">3</li>
					<li class="instruments-icon">4</li>
				</ul>
			</span>
		</div>
	</div>
	
	
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
	<script src="javascripts/main.js"></script>
	<script src="javascripts/myo.js"></script>
</body>
</html>